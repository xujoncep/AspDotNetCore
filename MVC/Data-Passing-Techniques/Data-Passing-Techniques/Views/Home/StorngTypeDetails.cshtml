@{
    Layout = null;
}
<!DOCTYPE html>
@model Data_Passing_Techniques.Models.Student

<html>
<head>
    <title>@ViewBag.Title</title>
</head>
<body style="text-align:center">
    <h1>Wellcome to @ViewBag.Title</h1>
    <div>
        StudentId : @Model?.StudentId
    </div>
    <div>
        Name : @Model?.Name
    </div>
    <div>
        Branch : @Model?.Branch
    </div>
    <div>
        Gender : @Model?.Gender
    </div>
</body>
</html>